        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "homepage": "https://github.com/elonehoo/keyboard#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/elonehoo/keyboard"
  },
  "bugs": "https://github.com/elonehoo/keyboard/issues",
  "scripts": {
    "build": "rimraf dist && tsup",
    "prepublishOnly": "pnpm run build"
  },
  "devDependencies": {
    "rimraf": "5.0.5",
    "tsup": "8.0.1",
    "typescript": "5.3.2"
  }
}
