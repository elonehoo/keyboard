{
  "name": "@elonehoo/keyboard",
  "type": "module",
  "version": "0.0.8",
  "description": "used to capture keyboard input.",
  "license": "MIT",
  "author": "Elone Hoo <huchengyea@163.com>",
  "keywords": [
    "keyboard"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
